@model List<Model>

<h1>Model Index</h1>

<div class="list-group">
    <ul class="list-group">
        @foreach (Model models in Model)
        {
                        <li class="list-group-item">
                            <a asp-controller="Car" asp-action="Index" asp-route-id="@models.Id">
                    @models.Name
                            </a>
                        </li>
        }
    </ul>
</div>