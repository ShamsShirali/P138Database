﻿@model List<Car>

<h1>Car Index</h1>

<div class="list-group">
    <ul class="list-group">
        @foreach (Car cars in Model)
        {
            <li class="list-group-item">
                        <a asp-controller="Car" asp-action="Detail" asp-route-id="@cars.Id">
                           @cars.Year
                        </a>
            </li>
        }
    </ul>
</div>